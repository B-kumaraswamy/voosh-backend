# Database
DATABASE_URL=postgresql://<USERNAME>@<HOST>:<PORT>/<DB_NAME>

# Redis
REDIS_URL=redis://localhost:6379

# Frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:5173

# Session and message settings
SESSION_TTL_SECONDS=86400
MAX_MESSAGES_REDIS=500

# RAG and LLM limits
RAG_MAX_TOKENS=1024
LLM_MAX_OUTPUT_TOKENS=1024
RAG_MAX_CONTEXT_CHARS=5000
LLM_CHUNK_SIZE=120
LLM_CHUNK_DELAY_MS=10

# Vector DB (Qdrant)
VECTOR_DB_PROVIDER=qdrant
QDRANT_URL=https://<YOUR_QDRANT_INSTANCE_URL>
QDRANT_COLLECTION=articles

# API Keys
GEMINI_API_KEY=your-gemini-api-key
JINA_API_KEY=your-jina-api-key

# Jina settings
JINA_EMBEDDING_MODEL=jina-embeddings-v3
JINA_API_URL=https://api.jina.ai/v1/embeddings

# Embedding dimensions
EMBED_DIM=1024

# Backend server port
PORT=4000
